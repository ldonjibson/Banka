  "scripts": {
    "start": "babel-node --require @babel/register --require @babel/polyfill ./server/index.js",
    "dev": "babel-watch server/index.js",
    "create_testdb": "node --require @babel/register --require @babel/polyfill ./server/models/dbcreate_test.js",
    "drop_testdb": "babel-node ./server/models/drop_testdb.js",
    "lint": "eslint ./ --fix",
    "test": "cross-env NODE_ENV=test nyc --require babel-register --require @babel/polyfill mocha test",
    "covtest": "nyc mocha --require @babel/register --require @babel/polyfill test/*.js",
    "test:coverage": "cross-env NODE_ENV=test nyc --require @babel/register _mocha  --exit",
    "coverage": "nyc --reporter=html --reporter=text --reporter=text-lcov mocha | coveralls"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "ignore-class-method": "methodToIgnore",
    "incude": [
      "**/*.js",
      "./*js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/coverage/**",
      "**/UI/**"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
